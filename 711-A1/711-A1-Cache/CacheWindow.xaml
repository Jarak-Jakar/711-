<Window x:Class="_711_A1.CacheWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_711_A1"
        mc:Ignorable="d"
        Title="Cache" Height="350" Width="525">
    <Grid>
        <DockPanel Margin="10">
            <Button x:Name="listFilesButton" Content="Get list of files on server" HorizontalAlignment="Left" Margin="0,278,0,0" VerticalAlignment="Top" Click="listFilesButton_Click"/>
            <ListView x:Name="filesListView" HorizontalAlignment="Left" Height="273" VerticalAlignment="Top" Width="497">
                <ListView.View>
                    <GridView>
                        <GridViewColumn/>
                    </GridView>
                </ListView.View>
            </ListView>

        </DockPanel>

    </Grid>
</Window>
